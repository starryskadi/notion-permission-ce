{
  "manifest_version": 3,
  "name": "Notion Permission",
  "description": "Notion Permission Chrome Extensions",
  "version": "1.0",
  "action": {
    "default_icon": "logo.png",
    "default_popup": "index.html"
  },
  "icons": {
    "48": "logo.png",
    "96": "logo.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://www.notion.so/*"]
    }
  ]
}
